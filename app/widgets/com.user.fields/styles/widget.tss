'.container': {
	width: '100%',
	height: 40,
	layout: 'horizontal'
}

'.left': {
	width: '30%'
}

'.right': {
	width: Ti.UI.FILL,
}

'#leftText': {
	left: 10,
	width: Ti.UI.FILL,
	textAlign: Ti.UI.TEXT_ALIGNMENT_LEFT,
	font: {
		fontFamily: 'HelveticaNeue-Bold',
		fontSize: 16
	}
}

'#rightText': {
	textAlign: Ti.UI.TEXT_ALIGNMENT_LEFT,
	width: Ti.UI.FILL,
	font: {
		fontFamily: 'HelveticaNeue-Light',
		fontSize: 12
	}
}
